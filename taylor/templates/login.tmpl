<article class="login">
<div class="login_image">
<img src="${top}/image/cabinet.png" class="login_icon">
</div>
<div class="login_form">
<form action="" method="post">
<fieldset>
<ul>
<input type="hidden" name="method" value="Login" id="id_method" />
<li>
<label for="id_username">UserName</label>
<input id="id_username" type="text" name="username" maxlength="255" />
</li>
<li>
<label for="id_password">Password</label>
<input id="id_password" type="password" name="password" maxlength="255" />
</li>
<li>
<div class="button">
<input id="home_login_btn" type="submit" value="Login">
</div>
</li>
</ul>
</fieldset>
</form>
</div>
</article>
